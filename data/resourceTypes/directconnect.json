    "conditionKeys": [
      "aws:ResourceTag/${TagKey}"
    ]
  },
  "dxlag": {
    "key": "dxlag",
    "arn": "arn:${Partition}:directconnect:${Region}:${Account}:dxlag/${LagId}",
    "conditionKeys": [
      "aws:ResourceTag/${TagKey}"
    ]
  },
  "dxvif": {
    "key": "dxvif",
    "arn": "arn:${Partition}:directconnect:${Region}:${Account}:dxvif/${VirtualInterfaceId}",
