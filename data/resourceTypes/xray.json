{
  "group": {
    "key": "group",
    "arn": "arn:${Partition}:xray:${Region}:${Account}:group/${GroupName}/${Id}",
    "conditionKeys": [
      "aws:ResourceTag/${TagKey}"
    ]
  },
  "sampling-rule": {
    "key": "sampling-rule",
    "arn": "arn:${Partition}:xray:${Region}:${Account}:sampling-rule/${SamplingRuleName}",
    "conditionKeys": [
      "aws:ResourceTag/${TagKey}"
    ]
  }
}