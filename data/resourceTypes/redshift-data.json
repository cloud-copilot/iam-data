{
  "cluster": {
    "key": "cluster",
    "arn": "arn:${Partition}:redshift:${Region}:${Account}:cluster:${ClusterName}",
    "conditionKeys": [
      "aws:ResourceTag/${TagKey}"
    ]
  },
  "workgroup": {
    "key": "workgroup",
    "arn": "arn:${Partition}:redshift-serverless:${Region}:${Account}:workgroup/${WorkgroupId}",
    "conditionKeys": [
      "aws:ResourceTag/${TagKey}"
    ]
  }
}