    "conditionKeys": [
      "aws:ResourceTag/${TagKey}"
    ]
  },
  "log-stream": {
    "key": "log-stream",
    "arn": "arn:${Partition}:logs:${Region}:${Account}:log-group:${LogGroupName}:log-stream:${LogStreamName}",
